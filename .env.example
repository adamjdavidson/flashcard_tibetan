# Environment Variables Template\n\n# Copy this to .env.local for local development\n# Copy to Vercel Environment Variables for deployment\n\n# Supabase Configuration\nVITE_SUPABASE_URL=your_supabase_project_url\nVITE_SUPABASE_ANON_KEY=your_supabase_anon_key\n\n# Supabase Service Role Key (for serverless functions only)\nSUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key\n\n# Translation API (optional)\nTRANSLATION_API_KEY=your_translation_api_key\nTRANSLATION_SERVICE=google\n\n# Image Generation API (optional)\nIMAGE_GENERATION_API_KEY=your_image_generation_api_key\nIMAGE_GENERATION_SERVICE=dalle\n\n# Unsplash API (optional)\nUNSPLASH_ACCESS_KEY=your_unsplash_access_key
